<div class="row">
  <h3 class="center-align">Category Listing </h3>
  <% @categories.each do |category| %>
    <div class="col s12 m6">
      <div class="card ">
        <div class="card-content ">
          <span class="card-title"><%= category.name %></span>
          
        </div>
        <div class="card-action">
        
        </div>
      </div>
    </div> 
    
  <% end %>
  <%= link_to '<i class="material-icons right">note</i>New'.html_safe, new_category_path, class:"waves-effect waves-light btn" %>
</div>