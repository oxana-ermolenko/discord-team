<div class="row sign-up-form">
  <div class="col s12 m6">
    <div class="card ">
      <div class="card-content ">
        <span class="card-title">Name: <%= @category.name %></span>
        
      </div>
      <div class="card-action">
      
      <%= link_to '<i class="material-icons right">delete</i>Delete'.html_safe, category_path(@category), method: :delete, class:"waves-effect waves-light btn" %>
      </div>
    </div>
  </div>  
</div>